@keyframes BodyblinkAnime{
	0%{ background-color: #F2F2F2; }
	100%{ background-color: #000000; }
}
@keyframes BodyblinkAnime_reset{
	0%{ background-color: #F2F200; }
	100%{ background-color: #000000; }
}
.input_txt{
	width: 50%;
	margin: 5px;
	padding: 5px 10px; /*ボックスを大きくする*/
	font-size: 16px;
	border-radius: 3px; /*ボックス角の丸み*/
	border: 2px solid #ddd; /*枠線*/
	box-sizing: border-box; /*横幅の解釈をpadding, borderまでとする*/
}
.input_txt::-webkit-inner-spin-button,
.input_txt::-webkit-outer-spin-button {
	-webkit-appearance: none;
	margin: 0;
	-moz-appearance:textfield;
}

::-webkit-scrollbar {
  width: 8px;
}
::-webkit-scrollbar&:hover {
  width: 20px;
}

::-webkit-scrollbar-track {
  border-radius: 4px;
  box-shadow: inset 0 0 12px rgba(0, 0, 0, 0.1);
}

::-webkit-scrollbar-thumb {
  background: linear-gradient(to top, #5bbee5 0%, #52ddae 100%);
  border-radius: 4px;
  box-shadow: 0 0 0 2px rgba(255, 255, 255, 0.3);
}



