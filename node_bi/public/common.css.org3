@keyframes blinkAnime{
	0%{ color: #000000; background-color: #FFFFFF; }
	100%{ color: #FFFFFF; background-color: #FF0000; }
}
#brink01 {
	animation: blinkAnime 1s infinite alternate;
	animation-play-state: paused;
}
#brink02 {
	animation: blinkAnime 1s infinite alternate;
	animation-play-state: paused;
}
#brink05 {
	animation: blinkAnime 1s infinite alternate;
	animation-play-state: paused;
	overflow-y: scroll;
	overflow-x: hidden;
}
